{
  "name": "app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prod:build": "babel ./app -d build",
    "prod:build:client": "webpack --config webpack.prod.js",
    "prod:build:server": "node ./build/server/index.js",
    "prod:start": "npm run prod:build && npm run prod:build:client && npm run prod:build:server",
    "dev:build:client": "webpack -w",
    "dev:hot:client": "node devServer.js",
    "dev:build:server": "nodemon -x babel-node ./app/server/index.js -w ./app/server/* ",
    "start": "npm-run-all -p dev:build:client dev:hot:client dev:build:server",
    "lint:js": "eslint src/ --ext .js,.jsx",
    "lint:js:fix": "eslint src/ --ext .js,.jsx --fix"
  },
  "author": "V.V.Arkavenko",
  "license": "MIT",
  "description": "Good client architecture",
  "browserslist": [
    "> 1%",
    "ie 10"
  ],
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "inline-react-svg",
      "@babel/plugin-syntax-dynamic-import",
      "react-loadable/babel"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint:js:fix",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-http-proxy": "^1.5.1",
    "i18next": "^17.0.6",
    "isomorphic-fetch": "^2.2.1",
    "query-string": "^6.8.3",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-helmet": "^5.2.0",
    "react-i18next": "^10.12.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.1.0",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.3.1",
    "react-select": "^3.0.4",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^1.5.0",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-node": "0.0.1-security",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "css-loader": "^3.0.0",
    "dotenv-webpack": "^1.7.0",
    "eslint-loader": "^2.2.1",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/node": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.1",
    "mini-css-extract-plugin": "^0.8.0",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-dev-server": "^3.8.0"
  }
}
